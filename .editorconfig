root = true

[*]
indent_style = space
indent_size = 4
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[pom.xml]
indent_style = space
indent_size = 2
insert_final_newline = false

[*.{yml}]
indent_style = space
indent_size = 2
insert_final_newline = false
